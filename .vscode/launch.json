    "version": "0.2.0",
    "configurations": [
        {
            "command": "npx react-native start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npx react-native run-ios",
            "name": "Run npm ios",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/App.js"
        }
    ]
}